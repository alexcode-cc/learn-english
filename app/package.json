{
  "name": "learn-english",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,js,jsx,json,css,scss,md}\"",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:component": "vitest run --config vitest.component.config.ts",
    "test:e2e": "playwright test",
    "test:perf": "lighthouse-ci",
    "convert:vocab": "node scripts/convert-vocab-to-csv.js",
    "check:vocab": "node scripts/convert-vocab-to-csv.js --check"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.6.7",
    "idb": "^8.0.0",
    "papaparse": "^5.4.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "fake-indexeddb": "^5.0.2",
    "jsdom": "^24.0.0",
    "playwright": "^1.42.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.19.0",
    "vitest": "^1.4.0",
    "workbox-window": "^7.0.0"
  }
}
